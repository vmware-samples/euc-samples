{
    "name": "cloudflare/cf-ip-rewrite",
    "description": "Library to rewrite CloudFlare IP Addresses to the end-user IP address",
    "version": "1.0.4",
    "homepage": "https://github.com/cloudflare/cf-ip-rewrite",
    "keywords": [
        "ip rewrite",
        "cloudflare"
    ],
    "authors": [
        {
            "name": "CloudFlare",
            "homepage": "http://www.cloudflare.com/"
        }
    ],
    "license": "MIT",
    "type": "library",
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml.dist",
        "format": "vendor/bin/phpcs -n --standard=PSR2 --extensions=php,live.php src/"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-0": {
            "CloudFlare\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "2.*"
    }
}
